{
  "id": "urban_transport_data",
  "engine": "playwright",
  "engineOptions": {
    "browser": "webkit",
    "args": [
      "--use-gl=desktop",
      "--enable-gpu",
      "--ignore-gpu-blocklist"
    ]
  },
  "viewports": [
    {
      "label": "desctop",
      "width": 1920,
      "height": 3264
    },
    {
      "label": "tablet",
      "width": 834,
      "height": 4008
    },
    {
      "label": "phone",
      "width": 375,
      "height": 5018
    }
  ],
  "scenarioDefaults": {
    "cookiePath": "backstop_data/engine_scripts/cookies.json",
    "url": "https://urbantransportdata.ru/news",
    "selectorExpansion": true,
    "misMatchThreshold" : 0.1,
    "requireSameDimensions": true
  },
  "scenarios": [
    {
      "label": "utd_news_main",
      "selectors": ["document"],
      "onReadyScript": "onlyScreen.js"
    },
    {
      "label": "utd_news_topics",
      "selectors": ["viewport"],
      "onReadyScript": "utd_news_topics.js",
      "viewports": [
        {
          "label": "desctop",
          "width": 1920,
          "height": 1080
        },
        {
          "label": "tablet",
          "width": 834,
          "height": 1194
        },
        {
          "label": "phone",
          "width": 375,
          "height": 768
        }
      ]
    },
    {
      "label": "utd_news_search",
      "url": "https://urbantransportdata.ru/news/search",
      "selectors": ["viewport"],
      "onReadyScript": "onlyScreen.js",
      "viewports": [
        {
          "label": "desctop",
          "width": 1920,
          "height": 1080
        },
        {
          "label": "tablet",
          "width": 834,
          "height": 1194
        }
      ]
    },
    {
      "label": "utd_news_login",
      "url": "https://urbantransportdata.ru/news",
      "selectors": ["viewport"],
      "onBeforeScript": "login.js",
      "onReadyScript": "onlyScreen.js",
      "viewports": [
        {
          "label": "desctop",
          "width": 1920,
          "height": 1080
        }
      ]
    },
    {
      "label": "utd_news_my-news",
      "url": "https://urbantransportdata.ru/news/my-news",
      "selectors": ["viewport"],
      "onBeforeScript": "login.js",
      "onReadyScript": "onlyScreen.js",
      "viewports": [
        {
          "label": "desctop",
          "width": 1920,
          "height": 1080
        }
      ]
    },
    {
      "label": "utd_news_my-news_delete",
      "url": "https://urbantransportdata.ru/news/my-news",
      "selectors": ["viewport"],
      "onBeforeScript": "login.js",
      "onReadyScript": "utd_news_my-news_delete.js",
      "viewports": [
        {
          "label": "desctop",
          "width": 1920,
          "height": 1080
        }
      ]
    },
    {
      "label": "utd_news_my-news_delete",
      "url": "https://urbantransportdata.ru/news/my-news",
      "selectors": ["viewport"],
      "onBeforeScript": "login.js",
      "onReadyScript": "utd_news_my-news_delete.js",
      "viewports": [
        {
          "label": "desctop",
          "width": 1920,
          "height": 1080
        }
      ]
    },
    {
      "label": "utd_news_my-news_redactor",
      "url": "https://urbantransportdata.ru/news/my-news",
      "selectors": ["viewport"],
      "onBeforeScript": "login.js",
      "onReadyScript": "utd_news_my-news_redactor.js",
      "viewports": [
        {
          "label": "desctop",
          "width": 1920,
          "height": 1080
        }
      ]
    },
    {
      "label": "utd_news_my-news_add",
      "url": "https://urbantransportdata.ru/news/my-news",
      "selectors": ["viewport"],
      "onBeforeScript": "login.js",
      "onReadyScript": "utd_news_my-news_add.js",
      "viewports": [
        {
          "label": "desctop",
          "width": 1920,
          "height": 1080
        }
      ]
    }
  ],
  "paths": {
    "bitmaps_reference": "backstop_data/webkit/bitmaps_reference/utd/news",
    "bitmaps_test": "backstop_data/webkit/bitmaps_test/utd/news",
    "engine_scripts": "backstop_data/webkit/engine_scripts/utd/news",
    "html_report": "backstop_data/webkit/html_report/utd/news",
    "ci_report": "backstop_data/webkit/ci_report/utd/news"
  },
  "report": ["browser"],
  "asyncCaptureLimit": 5,
  "asyncCompareLimit": 50,
  "debug": false,
  "debugWindow": false,
  "archiveReport": false,
  "scenarioLogsInReports": false
}